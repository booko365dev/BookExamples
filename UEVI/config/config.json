{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "hello-world-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/helloWorld/HelloWorldWebPart.js",
          "manifest": "./src/webparts/helloWorld/HelloWorldWebPart.manifest.json"
        }
      ]
    },
    "list-item-crud-java-script-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/listItemCrudJavaScript/ListItemCrudJavaScriptWebPart.js",
          "manifest": "./src/webparts/listItemCrudJavaScript/ListItemCrudJavaScriptWebPart.manifest.json"
        }
      ]
    },
    "list-item-crud-pn-p-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/listItemCrudPnP/ListItemCrudPnPWebPart.js",
          "manifest": "./src/webparts/listItemCrudPnP/ListItemCrudPnPWebPart.manifest.json"
        }
      ]
    },
    "other-pn-pjs-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/otherPnPjs/OtherPnPjsWebPart.js",
          "manifest": "./src/webparts/otherPnPjs/OtherPnPjsWebPart.manifest.json"
        }
      ]
    },
    "external-libraries-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/externalLibraries/ExternalLibrariesWebPart.js",
          "manifest": "./src/webparts/externalLibraries/ExternalLibrariesWebPart.manifest.json"
        }
      ]
    },
    "list-item-crud-graph-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/listItemCrudGraph/ListItemCrudGraphWebPart.js",
          "manifest": "./src/webparts/listItemCrudGraph/ListItemCrudGraphWebPart.manifest.json"
        }
      ]
    },
    "app-cust-extension-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/appCustExtension/AppCustExtensionApplicationCustomizer.js",
          "manifest": "./src/extensions/appCustExtension/AppCustExtensionApplicationCustomizer.manifest.json"
        }
      ]
    },
    "field-cust-extension-field-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/fieldCustExtension/FieldCustExtensionFieldCustomizer.js",
          "manifest": "./src/extensions/fieldCustExtension/FieldCustExtensionFieldCustomizer.manifest.json"
        }
      ]
    },
    "list-view-cust-extension-command-set": {
      "components": [
        {
          "entrypoint": "./lib/extensions/listViewCustExtension/ListViewCustExtensionCommandSet.js",
          "manifest": "./src/extensions/listViewCustExtension/ListViewCustExtensionCommandSet.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "https://code.jquery.com/jquery-3.4.1.min.js",
      "globalName": "jquery"
    }
  },
  "localizedResources": {
    "HelloWorldWebPartStrings": "lib/webparts/helloWorld/loc/{locale}.js",
    "ListItemCrudJavaScriptWebPartStrings": "lib/webparts/listItemCrudJavaScript/loc/{locale}.js",
    "ListItemCrudPnPWebPartStrings": "lib/webparts/listItemCrudPnP/loc/{locale}.js",
    "OtherPnPjsWebPartStrings": "lib/webparts/otherPnPjs/loc/{locale}.js",
    "ExternalLibrariesWebPartStrings": "lib/webparts/externalLibraries/loc/{locale}.js",
    "marked": "node_modules/marked/marked.min.js",
    "ListItemCrudGraphWebPartStrings": "lib/webparts/listItemCrudGraph/loc/{locale}.js",
    "AppCustExtensionApplicationCustomizerStrings": "lib/extensions/appCustExtension/loc/{locale}.js",
    "FieldCustExtensionFieldCustomizerStrings": "lib/extensions/fieldCustExtension/loc/{locale}.js",
    "ListViewCustExtensionCommandSetStrings": "lib/extensions/listViewCustExtension/loc/{locale}.js"
  }
}
